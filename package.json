{
  "name": "@beequeue/eslint-plugin",
  "version": "2.1.1",
  "description": "My opinionated, modular ESLint configs",
  "author": "BeeeQueue",
  "license": "MIT",
  "keywords": [
    "eslint",
    "config",
    "collection",
    "presets",
    "typescript",
    "react",
    "vue",
    "node"
  ],
  "repository": "github:BeeeQueue/eslint-plugin",
  "bugs": "https://github.com/BeeeQueue/eslint-plugin/issues",
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=16"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,json,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "tsc",
    "dev": "pnpm build --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "release": "pnpm build &&  changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^8.45.0",
    "prettier": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "@typescript-eslint/utils": "6.1.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "npm:eslint-plugin-i@2.27.5-4",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.0",
    "eslint-plugin-unicorn": "48.0.0",
    "eslint-plugin-vue": "9.15.1"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "link:.",
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.1",
    "@tsconfig/node16": "1.0.4",
    "@tsconfig/strictest": "2.0.1",
    "@types/eslint": "8.44.0",
    "@types/node": "16.18.38",
    "@typescript-eslint/rule-tester": "6.1.0",
    "eslint": "8.45.0",
    "eslint-define-config": "1.21.0",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "simple-git-hooks": "2.8.1",
    "type-fest": "4.0.0",
    "typescript": "5.1.6",
    "vitest": "0.33.0"
  }
}
