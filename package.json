{
  "name": "@beequeue/eslint-plugin",
  "version": "0.1.3",
  "description": "My opinionated, modular ESLint configs",
  "author": "BeeeQueue",
  "license": "MIT",
  "keywords": [
    "eslint",
    "config",
    "collection",
    "presets",
    "typescript",
    "react",
    "vue",
    "node"
  ],
  "repository": "github:BeeeQueue/eslint-plugin",
  "bugs": "https://github.com/BeeeQueue/eslint-plugin/issues",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,json,html,yml,yaml}": [
      "yarn prettier --write"
    ]
  },
  "scripts": {
    "build": "tsc",
    "dev": "yarn build --watch",
    "lint": "yarn eslint \"src/**/*.ts\"",
    "release": "yarn build && yarn changeset publish",
    "postinstall": "simple-git-hooks",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "peerDependencies": {
    "eslint": "^7.21.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-unicorn": "32.0.1",
    "eslint-plugin-vue": "7.9.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "workspace:*",
    "@changesets/changelog-github": "0.4.0",
    "@changesets/cli": "2.16.0",
    "@types/eslint": "7.2.11",
    "@types/node": "14.17.1",
    "eslint": "7.27.0",
    "lint-staged": "11.0.0",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "simple-git-hooks": "2.4.1",
    "type-fest": "1.1.3",
    "typescript": "4.2.4"
  }
}
