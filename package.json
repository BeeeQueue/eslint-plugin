{
  "name": "@beequeue/eslint-plugin",
  "version": "0.6.0",
  "description": "My opinionated, modular ESLint configs",
  "author": "BeeeQueue",
  "license": "MIT",
  "keywords": [
    "eslint",
    "config",
    "collection",
    "presets",
    "typescript",
    "react",
    "vue",
    "node"
  ],
  "repository": "github:BeeeQueue/eslint-plugin",
  "bugs": "https://github.com/BeeeQueue/eslint-plugin/issues",
  "packageManager": "pnpm@7.8.0",
  "engines": {
    "node": ">=14"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,json,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "tsc",
    "dev": "pnpm build --watch",
    "lint": "pnpm eslint \"src/**/*.ts\"",
    "release": "pnpm build && pnpm changeset publish",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^8",
    "prettier": "^2.6.2"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-n": "15.2.4",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-unicorn": "43.0.2",
    "eslint-plugin-vue": "9.0.1"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "link:.",
    "@changesets/changelog-github": "0.4.6",
    "@changesets/cli": "2.24.1",
    "@tsconfig/node14": "1.0.3",
    "@types/eslint": "8.4.5",
    "@types/node": "14.18.23",
    "eslint": "8.21.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "simple-git-hooks": "2.8.0",
    "type-fest": "2.18.0",
    "typescript": "4.7.4"
  }
}
