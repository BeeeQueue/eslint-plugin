{
  "name": "@beequeue/eslint-plugin",
  "version": "2.0.0",
  "description": "My opinionated, modular ESLint configs",
  "author": "BeeeQueue",
  "license": "MIT",
  "keywords": [
    "eslint",
    "config",
    "collection",
    "presets",
    "typescript",
    "react",
    "vue",
    "node"
  ],
  "repository": "github:BeeeQueue/eslint-plugin",
  "bugs": "https://github.com/BeeeQueue/eslint-plugin/issues",
  "packageManager": "pnpm@8.0.0-alpha.0",
  "engines": {
    "node": ">=16"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,json,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "tsc",
    "dev": "pnpm build --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "release": "pnpm build &&  changeset publish",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^8.34.0",
    "prettier": "^2.8.4"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint-plugin-vue": "9.9.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "link:",
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.0",
    "@tsconfig/node14": "1.0.3",
    "@types/eslint": "8.21.1",
    "@types/node": "14.18.36",
    "eslint": "8.34.0",
    "eslint-define-config": "1.15.0",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "simple-git-hooks": "2.8.1",
    "type-fest": "3.6.0",
    "typescript": "4.9.5"
  }
}
